{
  "name": "nunjucks-capture",
  "version": "1.0.4",
  "description": "A nunjucks port of the Liquid Capture tag",
  "main": "index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prettier": "prettier --single-quote --trailing-comma=es5 --bracket-spacing --tab-width 4 --write ./index.js `find ./test -name '*.js'`"
