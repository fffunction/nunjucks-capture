  },
  "keywords": [
    "nunjucks",
    "capture"
  ],
  "author": "Dan Reeves <danr@fffunction.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fffunction/nunjucks-capture.git"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "coveralls": "^3.0.0",
    "eslint-config-prettier": "^2.2.0",
    "nunjucks": "^3.0.1",
    "nyc": "^11.0.3",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">0.12"
  }
}
